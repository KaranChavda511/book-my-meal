{
  "name": "food-app-server",
  "version": "1.0.0",
  "description": "Backend for Food & Multi-Restaurant Booking Platform",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules src/server.ts",
    "migrate": "sequelize db:migrate --config src/utils/sequelizeConfig.js",
    "migrate:undo": "sequelize db:migrate:undo --config src/utils/sequelizeConfig.js",
    "seed": "sequelize db:seed:all --config src/utils/sequelizeConfig.js",
    "lint": "eslint . --ext .ts,.js"
  },
  "author": "you",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "eslint": "^9.33.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^8.1.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.3",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.32.1",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.2",
    "sequelize-cli": "^6.6.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.2"
  }
}
